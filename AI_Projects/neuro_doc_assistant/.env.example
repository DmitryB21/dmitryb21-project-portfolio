# Neuro_Doc_Assistant - Environment Variables Example
# Скопируйте этот файл в .env и заполните реальными значениями

# GigaChat API
GIGACHAT_API_KEY=your_gigachat_api_key_here
GIGACHAT_EMBEDDINGS_URL=https://api.gigachat.ai/v1/embeddings

# Qdrant
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=  # Опционально, если используется аутентификация

# PostgreSQL (для будущих модулей)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=neuro_doc_assistant
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password_here

# Embedding Configuration
EMBEDDING_MODEL_VERSION=GigaChat
EMBEDDING_DIM=1536

