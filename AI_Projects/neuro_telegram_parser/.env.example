# Telegram API credentials
TELEGRAM_API_ID=your_telegram_api_id
TELEGRAM_API_HASH=your_telegram_api_hash
TELEGRAM_PHONE_NUMBER=+1234567890

# PostgreSQL Database (локальный сервер)
# Формат: postgresql://user:password@host:port/database
POSTGRES_DSN=postgresql://indlab_user:indlab_pass@127.0.0.1:5432/indlab_db
POSTGRES_CUSTOMER_DSN=postgresql://indlab_user:indlab_pass@127.0.0.1:5432/telegram_data_customer

# Redis (локальный сервер)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=indlab_redis_pass

# Qdrant (локальный сервер)
QDRANT_HOST=127.0.0.1
QDRANT_PORT=6333

# OpenAI API (для эмбеддингов)
OPENAI_API_KEY=sk-your-openai-api-key



# Аутентификация и авторизация
# Секретный ключ для подписи JWT токенов (ОБЯЗАТЕЛЬНО измените в продакшене!)
SECRET_KEY=your_strong_secret_key_change_in_production_min_32_chars

# Пароль для дефолтного администратора (создается при первом запуске)
DEFAULT_ADMIN_PASSWORD=change_me_strong_password

# Email для дефолтного администратора (опционально)
DEFAULT_ADMIN_EMAIL=admin@example.com

# Время жизни JWT токена в минутах (по умолчанию 60 минут)
ACCESS_TOKEN_EXPIRE_MINUTES=60

